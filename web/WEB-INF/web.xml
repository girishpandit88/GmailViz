<?xml version="1.0"?>
<web-app xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee 
        http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd"
	version="2.4">
	<servlet>
		<servlet-name>home</servlet-name>
		<servlet-class>website.web.GmailServlet</servlet-class>
		<init-param>
			<param-name>JDBC_URL</param-name>
			<param-value>jdbc:oracle:thin:@ora.csc.ncsu.edu:1523:orcl</param-value>
		</init-param>
		<init-param>
			<param-name>user</param-name>
			<param-value>gapandit</param-value>
		</init-param>
		<init-param>
			<param-name>password</param-name>
			<param-value>001000715</param-value>
		</init-param>
	</servlet>
	<servlet>
		<servlet-name>folder</servlet-name>
		<servlet-class>website.web.FolderByPeriodServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ulreimp</servlet-name>
		<servlet-class>website.web.UserLastRepliedImportanceServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>userlastreplied</servlet-name>
		<servlet-class>website.web.UserLastReplyServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>contact</servlet-name>
		<servlet-class>website.web.SetContactImportanceServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>sendcontact</servlet-name>
		<servlet-class>website.web.SendContactServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>userbyyear</servlet-name>
		<servlet-class>website.web.GetUserDataByYearServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>userbymonth</servlet-name>
		<servlet-class>website.web.GetUserDataByMonthServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>userageimp</servlet-name>
		<servlet-class>website.web.GetAgevsImpCountServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>impcontact</servlet-name>
		<servlet-class>website.web.GetContactImportanceServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>impvage</servlet-name>
		<servlet-class>website.web.GetAgeVsCountIntervalsServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>keyword</servlet-name>
		<servlet-class>website.web.SetKeywordImportanceServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>home</servlet-name>
		<url-pattern>/home</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>contact</servlet-name>
		<url-pattern>/contact</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>sendcontact</servlet-name>
		<url-pattern>/sendcontact</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>impcontact</servlet-name>
		<url-pattern>/impcontact</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>userbyyear</servlet-name>
		<url-pattern>/userbyyear</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>userbymonth</servlet-name>
		<url-pattern>/userbymonth</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>keyword</servlet-name>
		<url-pattern>/keyword</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>folder</servlet-name>
		<url-pattern>/folder</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>impvage</servlet-name>
		<url-pattern>/impvage</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
	<servlet-name>userlastreplied</servlet-name>
	<url-pattern>/userlastreplied</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
	<servlet-name>ulreimp</servlet-name>
	<url-pattern>/ulreimp</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
	<servlet-name>userageimp</servlet-name>
	<url-pattern>/userageimp</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>
</web-app>